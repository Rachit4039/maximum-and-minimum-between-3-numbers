#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c;
    printf("input 3 numbers seperated by space\n");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b && a>c)
    {
        printf("a is greatest\n ");
        if (b>c)
            printf("c is smallest\n");
        else
            printf("b is smallest\n");
    }
    else if(b>c && b>a)
    {
         printf("b is greatest \n");
         if (a>c)
            printf("c is smallest\n");
        else
            printf("a is smallest\n");
    }
    else
    {
        printf("c is greatest\n ");
        if (a>b)
            printf("b is smallest\n");
        else
            printf("a is smallest\n");
    }
    return 0;
}
